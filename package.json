{
  "name": "notifly-js-sdk",
  "version": "1.1.1",
  "description": "Notifly JS SDK for browser.",
  "source": [
    "index.ts"
  ],
  "main": "dist/main.js",
  "browser": "dist/index.js",
  "types": "dist/index.d.ts",
  "targets": {
    "main": {
      "distDir": "dist"
    },
    "browser": {
      "context": "browser",
      "includeNodeModules": {
        "uuid": true
      },
      "engines": {
        "browsers": "> 0.25%"
      },
      "outputFormat": "esmodule"
    },
    "sw": {
      "distDir": "dist",
      "source": "NotiflyServiceWorker.ts"
    }
  },
  "scripts": {
    "test": "jest",
    "pretty": "prettier --write \"**/*.{js,jsx,json}\"",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "watch": "parcel watch",
    "build": "parcel build --no-source-maps",
    "start": "http-server --cors",
    "deploy-cdn": "aws s3 sync dist s3://notifly-content/notifly-js-sdk/dist && aws cloudfront create-invalidation --distribution-id E2M7VD2WC9S1MI --paths \"/notifly-js-sdk/dist/*\""
  },
  "author": "daeseongKim",
  "license": "ISC",
  "dependencies": {
    "@types/uuid": "^9.0.1",
    "localforage": "^1.10.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/plugin-syntax-import-assertions": "^7.20.0",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@parcel/packager-ts": "^2.9.1",
    "@parcel/transformer-typescript-types": "^2.9.1",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.3",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "http-server": "^14.1.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "parcel": "^2.9.1",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
